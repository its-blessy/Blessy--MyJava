import java.util.Scanner;
public class Array{
public static void main(String[] args) {

    Scanner sc = new Scanner(System.in);
    int size = sc.nextInt();
    int ECE[] = new int[size];
    for (int i=0;i<size;i++)
        ECE[i]=sc.nextInt();
    int min=ECE[0];
    for(int i=0;i<size;i++){
        if (min > ECE[i])
            min = ECE[i];
    }
    System.out.println("Minimum " + min);
}
    }

OUTPUT:
5 
12 34 56 30 11
Minimum 11


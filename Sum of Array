import java.util.Scanner;
import java.util.Arrays;
public class Main
{
	public static void main(String[] args) {
	
	Scanner kbd=new Scanner(System.in);
	int size=kbd.nextInt();
	int arr[]=new int[size];
	int sum=0;
	for(int i=0;i<size;i++){
	arr[i]=kbd.nextInt();
	sum+=arr[i];
	}
	System.out.print("Sum "+sum);
  }
}

OUTPUT:
5
1 2 3 4 5
Sum 15

